{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": ["eslint:recommended", "airbnb", "prettier", "prettier/react", "plugin:react/recommended"],
    "parser":"babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": ["react", "prettier"],
    "rules": {
        "react/jsx-filename-extension": [0],
        "react/jsx-uses-react": "error",
        "react/jsx-uses-vars": "error"
    }
}
